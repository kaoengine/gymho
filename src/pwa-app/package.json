    "react-swipeable-views": "^0.13.9",
    "styled-components": "^5.2.1"
  },
  "pwaManifest": {
    "name": "Gym at home",
    "shortName": "gymho",
    "startURL": "./offline",
    "theme": "#add8e6",
    "generateIconOptions": {
      "baseIcon": "./public/my-awesome-icon.svg",
